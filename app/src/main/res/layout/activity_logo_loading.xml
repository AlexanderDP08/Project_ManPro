<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/main_back"
    android:clipToOutline="true">
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="280dp"
        android:layout_height="280dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        app:layout_constraintBottom_toBottomOf="@+id/unknown_1_r"
        app:layout_constraintEnd_toEndOf="@+id/unknown_1_r"
        app:layout_constraintStart_toStartOf="@+id/unknown_1_r"
        app:layout_constraintTop_toTopOf="@+id/unknown_1_r"
        app:srcCompat="@drawable/ellipse_1" />

    <View
        android:id="@+id/unknown_1_r"
        android:layout_width="234dp"
        android:layout_height="216dp"
        android:layout_centerInParent="true"
        android:background="@drawable/logo_my_money"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>
